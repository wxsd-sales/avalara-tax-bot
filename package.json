{
  "name": "avalara-tax-bot",
  "version": "1.0.0",
  "description": "This project was bootstrapped with Fastify-CLI.",
  "main": "src/server.ts",
  "directories": {
    "test": "test"
  },
  "scripts": {
    "test": "npm run build && tsc -p test/tsconfig.json && tap --ts \"test/**/*.test.ts\"",
    "start": "npm run build && fastify start -l info dist/server.js",
    "build": "tsc && tsc-alias",
    "watch": "tsc && (concurrently \"tsc -w\" \"tsc-alias -w\")",
    "dev": "npm run build && concurrently -k -p \"[{name}]\" -n \"TypeScript,App\" -c \"yellow.bold,cyan.bold\" \"npm:watch\" \"npm:dev:start\"",
    "dev:start": "fastify start --ignore-watch=.ts -w -l info -P dist/server.js",
    "lint": "prettier --ignore-path .gitignore --plugin-search-dir . --check . && eslint --fix --ignore-path .gitignore .",
    "format": "prettier --ignore-path .gitignore --plugin-search-dir . --write ."
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@fastify/autoload": "^5.0.0",
    "@fastify/env": "^4.2.0",
    "@fastify/sensible": "^5.0.0",
    "@fastify/static": "^6.10.0",
    "as-table": "^1.0.55",
    "env-schema": "^5.2.0",
    "fastify": "^4.0.0",
    "fastify-cli": "^5.7.1",
    "fastify-plugin": "^4.0.0",
    "papaparse": "^5.4.1"
  },
  "devDependencies": {
    "@types/node": "^18.0.0",
    "@types/papaparse": "^5.3.7",
    "@types/tap": "^15.0.5",
    "@typescript-eslint/eslint-plugin": "^5.57.0",
    "@typescript-eslint/parser": "^5.57.0",
    "concurrently": "^7.0.0",
    "eslint": "^8.37.0",
    "eslint-config-prettier": "^8.8.0",
    "eslint-config-standard-with-typescript": "^34.0.1",
    "eslint-plugin-import": "^2.27.5",
    "eslint-plugin-n": "^15.7.0",
    "eslint-plugin-promise": "^6.1.1",
    "fastify-tsconfig": "^1.0.1",
    "prettier": "^2.8.7",
    "tap": "^16.1.0",
    "ts-node": "^10.4.0",
    "tsc-alias": "^1.8.5",
    "typescript": "^4.9.5"
  },
  "peerDependencies": {
    "@babel/runtime-corejs2": "^7.21.0",
    "webex": "^2.53.1"
  },
  "type": "commonjs",
  "engines": {
    "node": ">=18",
    "npm": ">=9"
  }
}
